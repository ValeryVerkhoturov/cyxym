{
  "version": 2,
  "name": "cyxym",
  "alias": "cyxym.now.sh",
  "routes": [
    {
      "src": "/*",
      "headers": {
        "Accept-Encoding": "br",
        "Strict-Transport-Security": "max-age=31536000",
        "X-Content-Type-Options": "nosniff",
        "Content-Security-Policy": "connect-src https:; object-src 'none'; frame-ancestors 'none'; form-action 'none'",
        "Referrer-Policy": "no-referrer",
        "X-XSS-Protection": "1; mode=block",
        "X-Frame-Options": "SAMEORIGIN"
      }
    }
  ]
}
